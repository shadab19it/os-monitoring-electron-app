{
  "name": "os-monitoring",
  "productName": "Os-Monitoring",
  "version": "2.4.2",
  "description": "A simple electron app for testing os-info",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "pack": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Shadab",
    "email": "shadab19it@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "monitor_one",
            "author": "Shadab"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shadab19it/Os-Monitoring"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.51",
    "@electron-forge/maker-deb": "6.0.0-beta.51",
    "@electron-forge/maker-rpm": "6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "6.0.0-beta.51",
    "@electron-forge/maker-zip": "6.0.0-beta.51",
    "@electron-forge/plugin-webpack": "6.0.0-beta.51",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "css-loader": "^3.0.0",
    "electron": "11.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "less": "^4.1.0",
    "less-loader": "^7.3.0",
    "node-loader": "^0.6.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@types/auto-launch": "^5.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/os-utils": "^0.0.1",
    "@types/react-router-dom": "^5.1.7",
    "antd": "^4.10.2",
    "auto-launch": "^5.0.5",
    "copy-webpack-plugin": "^7.0.0",
    "dotenv": "^8.2.0",
    "electron-log": "^4.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "os-utils": "^0.0.14",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "update-electron-app": "^2.0.1"
  }
}
